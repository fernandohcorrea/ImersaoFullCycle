  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run gRPC server",
      "type": "go",
      "request": "launch",
      "mode": "auto",
      "program": "${workspaceFolder}/cmd/main.go",
      "env": {
        "dbType": "postgres",
        "dsn": "dbname=codepix sslmode=disable user=postgres password=root host=db",
        "dbTypeTest": "sqlite3",
        "dsnTest": ":memory:",
        "env": "test",
        "debug": 1,
        "AutoMigrateDb": 1,
      }
    }
  ]
}